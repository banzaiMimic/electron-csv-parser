{
  "name": "electron-csv-parser",
  "version": "1.0.0",
  "description": "csv parser boilerplate using electron",
  "main": "src/index.js",
  "repository": "git@github.com:dr-mimic/electron-csv-parser.git",
  "license": "MIT",
  "scripts": {
